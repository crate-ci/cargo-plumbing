{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "ReadLockfileOut",
  "description": "Output messages for `cargo-plumbing read-lockfile`.",
  "oneOf": [
    {
      "type": "object",
      "properties": {
        "version": {
          "type": [
            "integer",
            "null"
          ],
          "format": "uint32",
          "minimum": 0
        },
        "reason": {
          "type": "string",
          "const": "lockfile"
        }
      },
      "required": [
        "reason"
      ]
    },
    {
      "description": "The locked package from the lockfile\n\n Expected to be outputted in a lexicographical order based on the package\n name, matching the order of the `[[package]]` entries in a `Cargo.lock`\n file.",
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "rev": {
          "type": [
            "string",
            "null"
          ]
        },
        "checksum": {
          "type": [
            "string",
            "null"
          ]
        },
        "dependencies": {
          "type": [
            "string",
            "null"
          ]
        },
        "replace": {
          "type": [
            "string",
            "null"
          ]
        },
        "reason": {
          "type": "string",
          "const": "locked-package"
        }
      },
      "required": [
        "reason",
        "id"
      ]
    },
    {
      "type": "object",
      "properties": {
        "unused": {
          "type": "array",
          "items": {
            "$ref": "#/$defs/NormalizedDependency"
          }
        },
        "reason": {
          "type": "string",
          "const": "unused-patches"
        }
      },
      "required": [
        "reason",
        "unused"
      ]
    }
  ],
  "$defs": {
    "NormalizedDependency": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "rev": {
          "type": [
            "string",
            "null"
          ]
        },
        "checksum": {
          "type": [
            "string",
            "null"
          ]
        },
        "dependencies": {
          "type": [
            "string",
            "null"
          ]
        },
        "replace": {
          "type": [
            "string",
            "null"
          ]
        }
      },
      "required": [
        "id"
      ]
    }
  }
}